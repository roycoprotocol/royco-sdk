{
  "name": "royco",
  "description": "SDK for building frontends on top of Royco Protocol",
  "version": "0.2.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "homepage": "https://royco.org",
  "repository": {
    "url": "https://github.com/roycoprotocol/royco-sdk"
  },
  "scripts": {
    "build": "tsup",
    "release": "pnpm run build && changeset publish",
    "lint": "tsc"
  },
  "keywords": [
    "royco"
  ],
  "author": "",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "import": "./dist/client/index.js",
      "require": "./dist/client/index.js",
      "types": "./dist/client/index.d.ts"
    },
    "./constants": {
      "import": "./dist/constants/index.js",
      "require": "./dist/constants/index.js",
      "types": "./dist/constants/index.d.ts"
    },
    "./hooks": {
      "import": "./dist/hooks/index.js",
      "require": "./dist/hooks/index.js",
      "types": "./dist/hooks/index.d.ts"
    },
    "./market": {
      "import": "./dist/market/index.js",
      "require": "./dist/market/index.js",
      "types": "./dist/market/index.d.ts"
    },
    "./provider": {
      "import": "./dist/provider/index.js",
      "require": "./dist/provider/index.js",
      "types": "./dist/provider/index.d.ts"
    },
    "./queries": {
      "import": "./dist/queries/index.js",
      "require": "./dist/queries/index.js",
      "types": "./dist/queries/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    }
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@supabase-cache-helpers/postgrest-react-query": "^1.6.2",
    "@supabase/postgrest-js": "^1.15.4",
    "@supabase/ssr": "^0.3.0",
    "@supabase/supabase-js": "^2.43.1",
    "@tanstack/react-query": "^5.35.1",
    "@tanstack/react-table": "^8.17.3",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@wagmi/core": "^2.10.3",
    "@weiroll/weiroll.js": "^0.3.0",
    "autoprefixer": "^10.4.19",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "ethers": "^5.4.4",
    "glob": "^10.4.1",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "react": "^18",
    "react-dom": "^18",
    "tailwindcss": "^3.4.3",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "viem": "^2.10.5",
    "wagmi": "^2.8.1"
  },
  "peerDependencies": {
    "@supabase/postgrest-js": "^1.15.4",
    "@supabase/ssr": "^0.3.0",
    "@tanstack/react-query": "^5.35.1",
    "@tanstack/react-table": "^8.17.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@wagmi/core": "^2.10.3",
    "ethers": "^5.4.4",
    "react": "^18",
    "react-dom": "^18",
    "typescript": "^5.4.5",
    "wagmi": "^2.8.1"
  },
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.0",
    "@supabase/postgrest-js": "^1.15.4",
    "@tanstack/react-query-devtools": "^5.61.3",
    "abitype": "^1.0.6",
    "cross-fetch": "^4.0.0",
    "lucide-react": "^0.379.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7"
  }
}
